{
  "openapi": "3.0.3",
  "info": {
    "title": "Torqlang Northwind DB API",
    "description": "This is an example API for the Northwind DB",
    "license": {
      "name": "Torqlang License v1.0",
      "url": "https://github.com/torqlang/torqlang/blob/main/LICENSE.md"
    },
    "version": "1.0"
  },
  "servers": [
    {
      "url": "https://northwind.torqlang.org/api/v1"
    }
  ],
  "tags": [
    {
      "name": "Orders",
      "description": "Orders"
    },
    {
      "name": "Purchase Orders",
      "description": "Purchase Orders"
    },
    {
      "name": "Products",
      "description": "Products"
    },
    {
      "name": "Other",
      "description": "Other"
    }
  ],
  "paths": {
    "/orders": {
      "get": {
        "tags": [
          "Orders"
        ],
        "responses": {
          "200": {
            "description": "Successful",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Order"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Order": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "employee-id": {
            "type": "integer",
            "format": "int64"
          },
          "customer-id": {
            "type": "integer",
            "format": "int64"
          },
          "order-date": {
            "type": "string",
            "format": "date-time",
            "example": "2006-01-15T00:00:00Z"
          },
          "shipped-date": {
            "type": "string",
            "format": "date-time",
            "example": "2006-01-22T00:00:00Z"
          },
          "shipper-id": {
            "type": "integer",
            "format": "int64"
          },
          "ship-name": {
            "type": "string",
            "maxLength": 50
          },
          "ship-address": {
            "type": "string",
            "format": "long-text"
          },
          "ship-city": {
            "type": "string",
            "maxLength": 50
          },
          "ship-state-province": {
            "type": "string",
            "maxLength": 50
          },
          "ship-zip-postal-code": {
            "type": "string",
            "maxLength": 50
          },
          "ship-country-region": {
            "type": "string",
            "maxLength": 50
          },
          "shipping-fee": {
            "type": "string",
            "format": "decimal(19, 4)"
          },
          "taxes": {
            "type": "string",
            "format": "decimal(19, 4)"
          },
          "payment_type": {
            "type": "string",
            "maxLength": 50
          },
          "paid-date": {
            "type": "string",
            "format": "date-time",
            "example": "2006-01-15T00:00:00Z"
          },
          "notes": {
            "type": "string",
            "format": "long-text"
          },
          "tax-rate": {
            "type": "number",
            "format": "double"
          },
          "tax-status-id": {
            "type": "integer",
            "format": "int32"
          },
          "status-id": {
            "type": "integer",
            "format": "int32"
          }
        }
      }
    }
  }
}